{
  "name": "ember-emotion",
  "description": "Work with emotion.js in Ember",
  "version": "5.0.0",
  "keywords": [
    "ember-addon",
    "style",
    "emotion",
    "scoped-styles"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexlafroscia/ember-emotion.git"
  },
  "license": "MIT",
  "author": {
    "name": "Alex LaFroscia",
    "email": "alex@lafroscia.com"
  },
  "directories": {
    "test": "tests"
  },
  "homepage": "http://alexlafroscia.com/ember-emotion/",
  "scripts": {
    "build": "ember build",
    "deploy": "ember build && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "standard-version"
  },
  "dependencies": {
    "babel-plugin-emotion": "^10.0.5",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.2",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-version-checker": "^2.1.2",
    "emotion": "^10.0.5",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.12.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-fastboot": "^2.0.0",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^1.10.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-code-snippet": "^2.3.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-head": "^0.1.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-qunit-assert-helpers": "^0.2.2",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.12.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-test-selectors": "^1.0.0",
    "ember-try": "^1.0.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-ember": "^6.1.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^1.15.3",
    "qunit-dom": "^0.8.4",
    "standard-version": "^4.4.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://alexlafroscia.com/ember-emotion",
    "fastbootDependencies": [
      "emotion"
    ]
  }
}
