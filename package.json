    "ember-addon",
    "style",
    "emotion",
    "scoped-styles"
  ],
  "license": "MIT",
  "author": {
    "name": "Alex LaFroscia",
    "email": "alex@lafroscia.com"
  },
  "homepage": "http://alexlafroscia.com/ember-emotion/",
